<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    
    <meta name="description" content="{{description}}">
    <meta name="keywords" content="{{keywords}}" />
    <meta name="viewport" content="width=device-width">

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link href="http://cdn-images.mailchimp.com/embedcode/slim-081711.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/css/normalize.css">
    <link rel="stylesheet" href="/css/main.css">
    <link href='http://fonts.googleapis.com/css?family=Lobster&subset=latin,cyrillic-ext' rel='stylesheet' type='text/css'>
    <script src="/js/vendor/modernizr-2.6.1.min.js"></script>
    <script src="/js/main.js"></script>
    <title>{{title}}</title>
  </head>
  <body>
    <!--[if lt IE 7]>
        <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
    <![endif]-->

    <!-- site content -->
    <header>
      <div id="header-content">
        <span class="lang"><a href="#">EN</a></span>
        <span class="lang"><a href="#">RU</a></span>

        <span id="logo">modern<br/>&nbsp;&nbsp;street photo</span>
        <span id="slogan">inspired by the streets</span>
        <div id="main-menu">
          {{#menu}}
            <ul>
              <li><a href="/" class="menu-item {{#home_active}}menu-item-active{{/home_active}}">{{home}}</a></li>
              <li><a href="/about" class="menu-item {{#about_active}} menu-item-active{{/about_active}}">{{event}}</a></li>
              <li><a href="/photographers" class="menu-item {{#photographers_active}} menu-item-active{{/photographers_active}}">{{photographers}}</a></li>
            </ul>
          {{/menu}}
        </div>
      </div>
    </header>
    <div id="our-smm">
          <span class="smm"><a href="#"><img src="../img/facebook.png" alt="facebook icon"/></a></span>
          <span class="smm"><a href="#"><img src="../img/twitter.png" alt="twitter icon"/></a></span>
          <span class="smm subscribe"><a href="#"><img src="../img/email.png" alt="subscribe icon"/></a></span>
        </div>
    
      <div id="content">

        {{{yield}}}
      </div><!-- /content -->
      <div id="mailchimp">
        <span class="close">Ã—</span>
        <!-- Begin MailChimp Signup Form -->
        <link href="http://cdn-images.mailchimp.com/embedcode/slim-081711.css" rel="stylesheet" type="text/css">
        <style type="text/css">
          #mc_embed_signup{background:#fff; clear:left; font:14px Helvetica,Arial,sans-serif;  width:400px;}
          /* Add your own MailChimp form style overrides in your site stylesheet or in this style block.
             We recommend moving this block and the preceding CSS link to the HEAD of your HTML file. */
        </style>
        <div id="mc_embed_signup">
        <form  id="mc-form" action="http://streetphoto.us6.list-manage1.com/subscribe/post?u=63451e08ba34ee805c275cb67&amp;id=f4b6d343c4" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
          <label for="mce-EMAIL">{{form_header}}</label>
          <input  style="width:100%;" type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="{{form_placeholder}}" required>
          <div class="clear"><input type="submit" value="{{form_submit}}" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
        </form>
        </div>

        <!--End mc_embed_signup-->
      </div>
    <footer>
      <div id="footer-content">
        <div id="feedback">
          {{{feedback}}}
        </div>

       
        <a class="my" href="http://vk.com/retgoat" title="{{vk_message}}"><img src="../img/my_vk.png" alt="{{vk_message}}"></a> 
        <a class="my" href="http://www.facebook.com/roman.sotnikov" title="{{fb_message}}"><img src="../img/my_fb.png" alt="{{fb_message}}"></a> 
        <a class="my" href="http://twitter.com/retgoat" title="twitter"></a> 
        <a class="my" href="http://500px.com/drove" title="{{px_message}}"><img src="../img/my_500px.png" alt="{{px_message}}"></a>
      
      </div>
    </footer>
    <div id="fader">   </div>
    

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.8.0.min.js"><\/script>')</script>
    <script src="../js/plugins.js"></script>
    <script src="../js/main.js"></script>

    <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
    <script>
      var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
